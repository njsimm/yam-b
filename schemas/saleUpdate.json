{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "saleUpdate Schema",
  "properties": {
    "id": {
      "type": "integer"
    },
    "productId": {
      "type": "integer"
    },
    "userId": {
      "type": "integer"
    },
    "quantitySold": {
      "type": "integer",
      "minimum": 1
    },
    "salePrice": {
      "type": "number",
      "minimum": 0
    },
    "saleDate": {
      "type": "string",
      "format": "date-time"
    },
    "updatedAt": {
      "type": "string",
      "format": "date-time"
    },
    "createdAt": {
      "type": "string",
      "format": "date-time"
    }
  },
  "examples": [
    {
      "id": 1,
      "productId": 123,
      "userId": 456,
      "quantitySold": 2,
      "salePrice": 19.99,
      "saleDate": "2024-05-30T10:00:00Z",
      "updatedAt": "2024-05-30T10:00:00Z",
      "createdAt": "2024-05-30T10:00:00Z"
    }
  ]
}
